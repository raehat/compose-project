<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.internshipt" >

    <application
        android:name=".FeedApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InternshipT" >
        <activity
            android:name=".Cards.CommentCard"
            android:exported="false"
            android:label="@string/title_activity_comment_card"
            android:theme="@style/Theme.InternshipT" />
        <activity
            android:name=".ui.CommentActivity"
            android:exported="false"
            android:label="@string/title_activity_comment"
            android:theme="@style/Theme.InternshipT" />
        <activity
            android:name=".ui.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.InternshipT" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>